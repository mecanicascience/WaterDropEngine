    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Trace engine",
            "cargo": {
                "args": [
                    "build",
                    "--bin=wde-engine",
                    "--package=wde-engine",
                    "--features=tracing"
                ],
                "filter": {
                    "name": "wde-engine",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug engine",
            "cargo": {
                "args": [
                    "build",
                    "--bin=wde-engine",
                    "--package=wde-engine"
                ],
                "filter": {
                    "name": "wde-engine",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Release engine",
            "cargo": {
                "args": [
                    "build",
                    "--release",
                    "--bin=wde-engine",
                    "--package=wde-engine"
                ],
                "filter": {
                    "name": "wde-engine",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}